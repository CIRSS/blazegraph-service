

repro.setenv HOME               ${HOME}
repro.setenv REPRO_MNT          /mnt/${REPRO_NAME}
repro.setenv PACKAGE_SNAPSHOT   ${PACKAGE_SNAPSHOT}
repro.setenv IN_RUNNING_REPRO   ${REPRO_NAME}
repro.setenv MODULES_DIR        ${MODULES_DIR}
repro.setenv BUNDLE_PATH_FILE   ${HOME}/.bundle_path

repro.atstart source ${DOT_ENV}
repro.atstart 'export PATH=$(<${BUNDLE_PATH_FILE}):$PATH'
repro.atstart cd ${REPRO_MNT}
