

repro.setenv HOME               ${REPRO_HOME}
repro.setenv REPRO_MNT          /mnt/${REPRO_NAME}
repro.setenv PACKAGE_SNAPSHOT   /repro/package
repro.setenv IN_RUNNING_REPRO   ${REPRO_NAME}
repro.setenv MODULES_DIR        ${HOME}/installed-modules

echo "export PATH=\`cat ${REPRO_HOME}/.bundle_path\`:\$PATH" >> ${REPRO_BASHRC}

echo "cd ${REPRO_MNT}" >> ${REPRO_BASHRC}
echo "start_blazegraph.sh" >> ${REPRO_BASHRC}
